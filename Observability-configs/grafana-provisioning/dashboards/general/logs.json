          "pluginVersion": "4.0.2",
          "queryType": "logs",
          "rawSql": "SELECT\r\n    toDateTime(Timestamp), \r\n    Concat(\r\n      substring(toString(Timestamp), 1, 23), '  ', \r\n      rightPad(SeverityText, 12), \r\n      '  ', ServiceName, '  ',\r\n  if(notEmpty(TraceId), concat(TraceId, '  '), ''),\r\n      if(empty('${Body:text}'), Body, replaceRegexpAll(Body, RegEx, '<b>\\\\0\\</b>')),\r\n      if (empty(LogAttributes['Logger']), '', concat(' | Logger = ', LogAttributes['Logger']) )\r\n    ) as Body,\r\n    SeverityText as level,\r\n    concat('((?i)', if(empty('${Body:text}'),' ', \r\n      replaceAll(\r\n        replaceAll(\r\n          replaceAll(\r\n            arrayStringConcat(splitByWhitespace(${Body:sqlstring}), '|'), \r\n            ')', '\\\\)'),\r\n          '(', '\\\\('),\r\n         '[','\\\\[') \r\n        ),')') as RegEx,\r\n\t\tTraceId\r\nFROM otel_logs\r\nWHERE\r\n    ServiceName in (${ServiceName:sqlstring})\r\n    AND(SeverityText in (${Level:sqlstring}))\r\n    AND(empty('${Body:text}') OR Body ILIKE '%${Body:text}%' OR toString(default.otel_logs.LogAttributes) ILIKE '%${Body:text}%')\r\n    AND(empty('${TraceId:text}') OR TraceId ILIKE replaceAll('%${TraceId:text}%', ' ', '%') OR toString(default.otel_logs.LogAttributes) ILIKE replaceAll('%${TraceId:text}%', ' ', '%'))\r\n    AND $__timeFilter(Timestamp)\r\nORDER BY Timestamp DESC\r\nLIMIT $Limit",
          "refId": "A"
        }
      ],
      "title": "Logs",
      "type": "logs"
    }
